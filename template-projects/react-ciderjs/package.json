{
  "name": "@gasbombe/react-ciderjs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:server": "rolldown -c && cpy server/*.json dist/",
    "build:front": "tsc -b && vite build",
    "build": "rimraf dist && pnpm run build:front && pnpm run build:server",
    "check": "biome check --write",
    "generate": "gasnuki",
    "test": "vitest run --coverage",
    "auth": "clasp-auth upload",
    "push": "clasp push",
    "deploy": "clasp create-deployment",
    "update": "dotenv cross-var \"clasp update-deployment $GAS_DEPLOYMENT_ID\"",
    "preview:local": "vite preview",
    "preview:web": "clasp open-web-app"
  },
  "dependencies": {
    "@ciderjs/city-gas": "catalog:",
    "@ciderjs/gasnuki": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@ciderjs/clasp-auth": "catalog:",
    "@google/clasp": "catalog:",
    "@types/google-apps-script": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "cpy-cli": "catalog:",
    "cross-var": "catalog:",
    "dotenv-cli": "catalog:",
    "rimraf": "catalog:",
    "rolldown": "catalog:",
    "rolldown-plugin-remove-export": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-google-apps-script": "catalog:",
    "vite-plugin-singlefile": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  }
}
